{
    "general":{
        "device": "cpu",
        "foldseek": "foldseek",
        "dssp": "mkdssp",
        "np_random": 0

    },
    "data": {
        "default": "receptors",
        "receptors": {
            "pdb_list": "data/receptors.txt",
            "folder_name": "receptors"
        },
        "mega-batch": {
            "pdb_list": "data/mega-batch20K.txt",
            "folder_name": "mega-batch"
        }
    },
    "labels": {
        "default": "dssp",
        "dssp": {
            "method": "dssp",
            "save_folder": "labels/dssp",
            "raw_folder": "raw/dssp",
            "abbreviation": "ss",
            "classes": ["H", "B" ,"G" ,"I" ,"P" ,"E" ,"T", "S" ,"#"]
        },
        "sasa-res": {
            "method": "sasa",
            "save_folder": "labels/sasa_res",
            "abbreviation": "sasa",
            "scope": "R",
            "probe-radius": 1.40,
            "threshold": 20.0,
            "classes": ["I", "O" ,"C" ,"#"]
        }
    },
    "embeddings": {
      "default": "SaProt-full",
        "SaProt-full":{
            "model": "SaProt",
            "mode": "full",
            "dimensions": 480,
            "save_folder": "embeddings/SaProt_full",
            "foldseek_folder": "raw/foldseek"
        },
        "SaProt-seq": {
            "model": "SaProt",
            "mode": "seq",
            "dimensions": 480,
            "save_folder": "embeddings/SaProt_seq"
        }
    },

    "training": {
        "default": "MLP-20",
        "MLP-10": {
            "model": "MLP",
            "embeddings": "SaProt-full",
            "epochs":100,
            "batch_size": 32,
            "shuffle": false,
            "test_ratio": 0.2
        },
        "MLP-20": {
            "model": "MLP",
            "epochs": 20,
            "batch_size": 64,
            "shuffle": false,
            "test_ratio": 0.2
        },
        "MLP-50": {
            "model": "MLP",
            "epochs": 50,
            "batch_size": 128,
            "shuffle": false,
            "test_ratio": 0.2
        }
    },
    "predict": {

    },
    "setups": {
        "default": "internship-receptors",
        "internship-large": {
            "data": "mega-batch",
            "labels": "dssp",
            "embeddings": "SaProt-full",
            "training": "MLP-10"
        },
        "internship-receptors": {
            "data": "receptors",
            "labels": "dssp",
            "embeddings": "SaProt-full",
            "training": "MLP-20"
        },
        "receptor-sasa": {
            "data": "receptors",
            "labels": "sasa-res",
            "embeddings": "SaProt-full",
            "training": "MLP-20"
        },
        "mega-sasa": {
                    "data": "mega-batch",
                    "labels": "sasa-res",
                    "embeddings": "SaProt-full",
                    "training": "MLP-20"
                }
    }
}