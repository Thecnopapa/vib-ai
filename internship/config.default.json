{
    "general":{
        "device": "cpu",
        "foldseek": "foldseek",
        "dssp": "mkdssp",
        "np_random": 0

    },
    "data": {
        "default": "receptors",
        "receptors": {
            "pdb_list": "data/receptors.txt",
            "folder_name": "receptors"
        },
        "mega-batch": {
            "pdb_list": "data/mega-batch20K.txt",
            "folder_name": "mega-batch"
        }
    },

    "embeddings": {
        "SaProt_full":{
            "model": "SaProt",
            "mode": "full",
            "dimensions": 480,
            "save_folder": "out/SaProt/full"
        },
        "SaProt_seq": {
            "model": "SaProt",
            "mode": "seq",
            "dimensions": 480,
            "save_folder": "out/SaProt/seq"
        }
    },
    "labels": {
        "dssp": {
            "folder": "out/SaProt/full"
        }
    },
    "training": {
        "SaProt_full": {
            "embeddings": "SaProt_full",
            "epochs": 20,
            "batch_size": 32,
            "shuffle": false,
            "test_ratio": 0.2
        },
        "SaProt_seq": {
            "embeddings": "SaProt_seq",
            "epochs": 20,
            "batch_size": 32,
            "shuffle": false,
            "test_ratio": 0.2
        }
    },
    "predict": {

    }
}